    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^1.13.3",
    "eslint-config-next": "^12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "feed": "^4.2.2",
    "globby": "^12.0.2",
