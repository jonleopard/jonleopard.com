    "contentful": "^9.1.4",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "markdown": "^0.5.0",
    "next": "^12.0.3",
    "next-plausible": "^3.1.4",
    "next-seo": "^4.28.1",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-calendly": "^2.2.2",
    "react-dom": "^17.0.2",
