{
  "name": "jonleopard.com",
  "version": "6.0.0",
  "private": true,
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "scripts": {
    "lint": "eslint ./src/pages/* ./src/components/* ./src/lib/* -c .eslintrc.json --fix --ext ts,tsx",
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "start": "next start",
